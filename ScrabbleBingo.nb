Cell[BoxData[
 RowBox[{
  RowBox[{"ScrabbleBingo", "[", "rack_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "sevens", ",", "sortedCharSevens", ",", "bingoPositions", ",", "letters",
       ",", "possibleRacks", ",", "blankPos"}], "}"}], ",", 
    RowBox[{
     RowBox[{"sevens", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"dictionary", ",", 
        RowBox[{
         RowBox[{
          RowBox[{"StringLength", "[", "#", "]"}], "==", "7"}], "&"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sortedCharSevens", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Sort", ",", 
        RowBox[{"Characters", "[", 
         RowBox[{"ToUpperCase", "[", "sevens", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"letters", "=", 
      RowBox[{"CharacterRange", "[", 
       RowBox[{"\"\<A\>\"", ",", "\"\<Z\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t", "\[IndentingNewLine]", "   ", 
     RowBox[{"blankPos", "=", 
      RowBox[{"StringPosition", "[", 
       RowBox[{"rack", ",", "\"\<?\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t", "\[IndentingNewLine]", "    ", 
     RowBox[{"Switch", "[", 
      RowBox[{
       RowBox[{"Length", "[", "blankPos", "]"}], ",", "\[IndentingNewLine]", 
       "        ", "0", ",", 
       RowBox[{"(*", " ", 
        RowBox[{"If", " ", "there", " ", "are", " ", "no", " ", 
         RowBox[{"blanks", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
       "        ", 
       RowBox[{"possibleRacks", "=", 
        RowBox[{"{", "rack", "}"}]}], ",", "\[IndentingNewLine]", "        ", 
       "1", ",", 
       RowBox[{"(*", " ", 
        RowBox[{"If", " ", "there", " ", "is", " ", "one", " ", 
         RowBox[{"blank", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
       "        ", 
       RowBox[{"possibleRacks", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"StringReplacePart", "[", 
           RowBox[{"rack", ",", "#", ",", 
            RowBox[{"blankPos", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "&"}], "/@", 
         "letters"}]}], ",", "\[IndentingNewLine]", "        ", "2", ",", 
       RowBox[{"(*", " ", 
        RowBox[{"If", " ", "there", " ", "are", " ", "two", " ", 
         RowBox[{"blanks", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
       "        ", 
       RowBox[{"possibleRacks", "=", 
        RowBox[{"Flatten", "[", 
         RowBox[{"Outer", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"StringReplacePart", "[", 
             RowBox[{"rack", ",", 
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}], ",", "blankPos"}], "]"}], 
            "&"}], ",", "letters", ",", "letters", ",", "1"}], "]"}], 
         "]"}]}]}], "\[IndentingNewLine]", "    ", "]"}], ";", 
     "\[IndentingNewLine]", "\t\t", "\[IndentingNewLine]", "    ", 
     RowBox[{"bingoPositions", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"sortedCharSevens", ",", 
           RowBox[{"Sort", "[", 
            RowBox[{"Characters", "[", 
             RowBox[{"ToUpperCase", "[", "#", "]"}], "]"}], "]"}]}], "]"}], 
         "&"}], "/@", "possibleRacks"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\t", "\[IndentingNewLine]", "    ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "bingoPositions", "]"}], ">", "0"}], ",", 
       "\[IndentingNewLine]", " ", 
       RowBox[{"DeleteDuplicates", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"ToUpperCase", "[", 
           RowBox[{"sevens", "[", 
            RowBox[{"[", "#", "]"}], "]"}], "]"}], "&"}], "/@", 
         "bingoPositions"}], "]"}], ",", "\[IndentingNewLine]", "        ", 
       RowBox[{"Print", "[", "\"\<No Scrabble Bingos\>\"", "]"}]}], 
      "\[IndentingNewLine]", "    ", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
